@Model List<Advisor>

    <!-- Team Start -->
    <div class="team mt-125">
        <div class="container">
            <div class="section-header">
                <p>Meet Our Advisors</p>
                <h2>Our Professional Consulting Team</h2>
            </div>
            <div class="row">
                @foreach (var advisor in Model)
                {

                    <div class="col-lg-3 col-md-6">
                        <div class="team-item">
                            <div class="team-img">
                                <img src="~/img/@advisor.Image" alt="Team Image">
                            </div>
                            <div class="team-text">
                                <h2>@advisor.Name</h2>
                                <p>@advisor.Profession</p>
                                <div class="team-social">
                                    <a href="@advisor.TwitterUrl"><i class="fab fa-twitter"></i></a>
                                    <a href="@advisor.FacebookUrl"><i class="fab fa-facebook-f"></i></a>
                                    <a href="@advisor.LinkedinUrl"><i class="fab fa-linkedin-in"></i></a>
                                    <a href="@advisor.InstagramUrl"><i class="fab fa-instagram"></i></a>
                                </div>
                            </div>
                        </div>
                    </div>
                }

            </div>
        </div>
    </div>
    <!-- Team End -->
