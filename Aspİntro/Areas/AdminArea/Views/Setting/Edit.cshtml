@model Setting
@{
    ViewData["Title"] = "Edit";
    Layout = "~/Areas/AdminArea/Views/Shared/_AdminLayout.cshtml";

}

<div class="container">
    <div class="row">
        <div class="col-md-6 grid-margin stretch-card">
            <div class="card">
                <div class="card-body">
                    <h4 class="card-title">Consulting Team</h4>
                    <p class="card-description"> Basic form layout </p>
                    <div asp-validation-summary="All"></div>
                    <form class="forms-sample" method="post" asp-action="Edit" enctype="multipart/form-data">
                        <div class="form-group">
                            <label asp-for="Key">Key</label>
                            <input class="form-control" placeholder="Key" asp-for="Key">
                            <span class="text-danger" asp-validation-for="Key"></span>

                            <label asp-for="Value">Value</label>
                            <input class="form-control" placeholder="Value" asp-for="Value">
                            <span class="text-danger" asp-validation-for="Value"></span>

                        </div>
                        <button type="submit" class="btn btn-primary mr-2">Submit</button>
                        <button asp-action="index" class="btn btn-light">Cancel</button>
                    </form>
                </div>
            </div>
        </div>
    </div>
</div>

@section Scripts{
    <partial name="_ValidationPartial" />
}